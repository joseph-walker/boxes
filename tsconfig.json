{
	"include": [
		"./src"
	],
	"exclude": [
		"node_modules"
	],
	"compilerOptions": {
		"target": "es6",
		"lib": ["es6"],
		"declaration": true,
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "./dist",
		"baseUrl": "src",
		"paths": {
			"guard/*": ["guard/*"],
			"algebra/*": ["algebra/*"]
		}
	}
}
